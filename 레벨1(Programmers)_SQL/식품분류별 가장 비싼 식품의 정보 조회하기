SELECT
    MAIN_TB.CATEGORY,
    MAIN_TB.PRICE,
    MAIN_TB.PRODUCT_NAME
FROM
    FOOD_PRODUCT MAIN_TB
INNER JOIN
    (SELECT
        CATEGORY,
        MAX(PRICE) AS MAX_PRICE
    FROM
        FOOD_PRODUCT
    WHERE
        CATEGORY IN ('과자', '국', '김치', '식용유')
    GROUP BY CATEGORY) SUB_TB
ON MAIN_TB.CATEGORY = SUB_TB.CATEGORY
AND MAIN_TB.PRICE = SUB_TB.MAX_PRICE
ORDER BY MAX_PRICE DESC
